	helpCommand		->	Type
	helpSectionDelim	->	PreProc
	HelpHeadline		->	Statement

	# python
	pythonBytesEscape		softred		none
	pythonStrFormat			softred		none
	pythonStrFormatting		softred		none

	# c/cpp
	cSpecial			softred		none
	cBraces			->	NonText

	#if get(g:Vsd, 'conservative', 0)
		cStorageClass	->	Type
		cppType		->	Type
		cppStructure	->	Type
	#else
		cStorageClass	->	Keyword
		cppType		->	Keyword
		cppStructure	->	Keyword
	#endif
#endif

