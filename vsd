let g:fzf_colors = {
	\ "fg":      ["fg", "Normal"],
	\ "bg":      ["bg", "Normal"],
	\ "hl":      ["fg", "Conditional"],
	\ "fg+":     ["fg", "CursorLine", "CursorColumn", "Normal"],
	\ "bg+":     ["bg", "CursorLine", "CursorColumn"],
	\ "hl+":     ["fg", "Conditional"],
	\ "info":    ["fg", "Conditional"],
	\ "border":  ["fg", "Ignore"],
	\ "prompt":  ["fg", "Comment"],
	\ "pointer": ["fg", "Conditional"],
	\ "marker":  ["fg", "Conditional"],
	\ "spinner": ["fg", "Conditional"],
	\ "header":  ["fg", "Conditional"]
	\}
