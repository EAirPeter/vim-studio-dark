# extra highlight {{{2

#if get(g:Vsd, 'extra_highlight', 1)
	# gutter
	GitGutterAdd			green		none
	GitGutterChange			darkyellow	none
	GitGutterChangeDelete		darkyellow	none
	GitGutterDelete			red		none
